  <xacro:macro name="ensenso_X200_Series" params="name margin:=0.03 radius_lens_sensor:=0.02 length_lens_sensor:=0.05 radius_lens_projector:=0.02 length_lens_projector:=0.05">

    <link name="${name}">
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://ensenso_description/stl_meshes/X3x-5CP-200.stl"/>
        </geometry>
        <material name="Ensenso/Blue"/>
      </visual>
      <collision>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0005 0.033"/>
        <geometry>
          <box size="${margin+0.24350} ${margin+0.08} ${margin+0.066}"/>
        </geometry>
      </collision>
    </link>

    <!-- Left lens -->
    <joint name="${name}_left_lens_joint" type="fixed">
      <parent link="${name}"/>
      <child link="${name}_left_lens_frame"/>
      <origin xyz="-0.1067 0.036 0.033" rpy="-1.570796 0 0"/>
    </joint>
    <xacro:objective name="${name}" margin="${margin}" identifier="left" radius="${radius_lens_sensor}" length_tube="${length_lens_sensor}"/>

    <!-- Right lens -->
    <joint name="${name}_right_lens_joint" type="fixed">
      <parent link="${name}"/>
      <child link="${name}_right_lens_frame"/>
      <origin xyz="0.1067 0.036 0.033" rpy="-1.570796 0 0"/>
    </joint>
    <xacro:objective name="${name}" margin="${margin}" identifier="right" radius="${radius_lens_sensor}" length_tube="${length_lens_sensor}"/>

    <!-- Projector lens -->
    <joint name="${name}_projector_lens_joint" type="fixed">
      <parent link="${name}"/>
      <child link="${name}_projector_lens_frame"/>
      <origin xyz="0 0.04 0.033" rpy="-1.570796 0 0"/>
    </joint>
    <xacro:objective name="${name}" margin="${margin}" identifier="projector" radius="${radius_lens_projector}" length_tube="${length_lens_projector}"/>
  </xacro:macro>


