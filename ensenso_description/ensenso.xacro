  <xacro:macro name="objective" params="radius length_tube *origin">
      <visual>
        <xacro:insert_block name="origin" />
        <geometry>
          <cylinder length="${length_tube}" radius="${radius}"/>
        </geometry>
        <material name="Ensenso/Black"/>
      </visual>
  </xacro:macro>

