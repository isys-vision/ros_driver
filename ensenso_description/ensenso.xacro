  <xacro:macro name="ensenso_X200_Series" params="name margin:=0.03 radius_lens_sensor:=0.02 length_lens_sensor:=0.05">

    <link name="${name}">
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="package://ensenso_description/stl_meshes/X3x-5CP-200.stl"/>
        </geometry>
        <material name="Ensenso/Blue"/>
      </visual>
      <xacro:objective radius="${radius_lens_sensor}" length_tube="${length_lens_sensor}">
        <origin xyz="-0.1067 ${0.036+length_lens_sensor/2.0} 0.033" rpy="${-pi/2} 0 0"/>
      </xacro:objective>
      <xacro:objective radius="${radius_lens_sensor}" length_tube="${length_lens_sensor}">
        <origin xyz="0.1067 ${0.036+length_lens_sensor/2.0} 0.033" rpy="-1.570796 0 0"/>
      </xacro:objective>
      <xacro:objective radius="${radius_lens_sensor}" length_tube="${length_lens_sensor}">
        <origin xyz="0 ${0.04+length_lens_sensor/2.0} 0.033" rpy="-1.570796 0 0"/>
      </xacro:objective>

      <collision>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 ${0.0005+length_lens_sensor/2} 0.033"/>
        <geometry>
          <box size="${margin+0.24350} ${margin+0.08+length_lens_sensor} ${margin+0.066}"/>
        </geometry>
      </collision>
    </link>

    <!-- Left lens -->
    <joint name="${name}_left_lens_joint" type="fixed">
      <parent link="${name}"/>
      <child link="${name}_left_lens_frame"/>
      <origin xyz="-0.1067 0.036 0.033" rpy="-1.570796 0 0"/>
    </joint>
    <link name="${name}_left_lens_frame"/>

  </xacro:macro>


